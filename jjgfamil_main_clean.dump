INSERT INTO `famille_maj` VALUES (4,'Blog \"bonne idée\"','Ajout par Amélie d\'une nouvelle catégorie, \"Blog JJG\", dans les liens.  Ajout dans cette rubrique du blog \"Bonne idée\".','c6bf36fa89',1126376989,1124402400,214,'Amélie','liens/view_cate.php?cate_id=5',0,1);
INSERT INTO `famille_maj` VALUES (5,'Association famille musik à la recherche de membres.','L\'association famille musik est à la [b:0222cda3eb]recherche de nouveaux membres[/b:0222cda3eb] pour l\'aider à financer un nouvel hébergement. L\'espace supplémentaire sera utilisé entres autres pour rajouter de nouveaux extraits audios/vidéos','0222cda3eb',1126863633,1125871200,214,'Amélie','famille/rub.php?rub_id=20',0,1);
INSERT INTO `famille_maj` VALUES (6,'Site officieux des Enfoirés','Ajout par Amélie du site \"Les Enfoirés, le site officieux\", dans la catégorie \"Enfoirés\" de la rubrique Liens. Site très complet, à voir !','b03083ce89',1127058820,1125352800,214,'Amélie','liens/view_cate.php?cate_id=3',0,1);
INSERT INTO `famille_maj` VALUES (7,'RDF parisienne du 22/10/2005 :D','Une rdf parisienne s\'organise pour le samedi 22 octobre 2005','a572cb1c89',1127213006,1127340000,725,'Goldmanikou','famille/view_rdf.php?rdf_id=19',0,1);
INSERT INTO `famille_maj` VALUES (8,'Groupes d\'utilisateurs','Maintenant, vous pouvez vous inscrire dans des groupes d\'utilisateurs correspondant à votre région géographique. Lorsqu\'une RDF se préparera dans cette région, vous serez ainsi automatiquement avertis. \r\nPour plus d\'informations rendez-vous [famurl=http://www.jjgfamille.com/forum/ftopic2284-nouveau__groupes_d_utilisateurs.html][b:c673aae43f]ICI[/b:c673aae43f][/famurl]','c673aae43f',1127221238,1125698400,725,'Goldmanikou','forum/groupcp.php',0,1);
INSERT INTO `famille_maj` VALUES (9,'Galerie Photos > Les enfoirés','Goldmanikou a ouvert une nouvelle categorie de photos intitulée \"Les enfoirés\"','fd40758cf6',1127244950,1125525600,725,'Goldmanikou','jjg/photos_view_cate.php?cate_id=38',0,1);
INSERT INTO `famille_maj` VALUES (10,'L\'aide du chat','Boris a mis à jour l\'aide du chat pour les futures soirées chat du mercredi et notemment la procédure d\'installation du x-chat.','847b3937a8',1127753730,1126303200,725,'Goldmanikou','chat/help.php',0,1);
INSERT INTO `famille_maj` VALUES (11,'Site \"l\'Ange de l\'Enfer\".','Ajout par Amélie d\'une nouvelle catégorie dans les liens , \"Ses interprètes\", consacrée aux artistes pour lesquels JJG a écrit des chansons. Ajout dans cette catégorie du site \"L\'Ange de l\'enfer\", consacré à Garou et ses musiciens. ','e26c04a172',1127802784,1126303200,214,'Amélie','liens/view_cate.php?cate_id=6',0,1);
INSERT INTO `famille_maj` VALUES (12,'Galerie Photos','Ajout de nouvelles photos dans la galerie photos de famille notemment dans les catégories \"JJG\" et \"Les enfoirés\"','0a40e5aa6c',1127934420,1128117600,725,'Goldmanikou','jjg/photos.html',0,1);
INSERT INTO `famille_maj` VALUES (13,'Patrick Fiori','Le dernier album de Patrick Fiori [b:177189ae5b]Si on chantait plus fort[/b:177189ae5b] a été ajouté par [b:177189ae5b]nico[/b:177189ae5b] dans le discographie de famille','177189ae5b',1127977633,1128376800,725,'Goldmanikou','disco/goldman_album_533_si_on_chantait_plus_fort.html',0,1);
INSERT INTO `famille_maj` VALUES (14,'Bibliotheque','Les livres \"8 accompagnements pour le MPOCH n°1\" et \"Ensemble, la pluie\" ont été ajouté dans la categorie \"Partitions\". De plus des editions et illustrations ont été ajouté/modifié. Tout ceci par le reponsable de cette rubrique j\'ai nommé [b:1a43822d06]Olivier[/b:1a43822d06]','1a43822d06',1128004327,1128981600,725,'Goldmanikou','jjg/bibliographie.html',0,1);
INSERT INTO `famille_maj` VALUES (15,'Patrick Fiori : les chansons','Ajout par Boris des chansons (avec toutes leurs références) écrites et/ou composées par JJG sur le nouvel album de Patrick Fiori. ','b5018811b8',1128580757,1129759200,214,'Amélie','disco/goldman_participations.html',0,1);
INSERT INTO `famille_maj` VALUES (17,'Revue du Net','Une nouvelle revue du net a été ajoutée par nico. Pour la recevoir par mail et pour vous inscrire [famurl=http://www.jjgfamille.com/actu/rdn.html]cliquez ici[/famurl]','a779f050df',1129577310,1130104800,725,'Goldmanikou','actu/rdn.html',0,1);
INSERT INTO `famille_maj` VALUES (18,'Expiration des comptes.','Le 1er novembre, un nettoyage sera effectué dans les comptes d\'utilisateurs. Tout compte qui ne se sera pas confirmé sera automatiquement supprimés ! Rendez-vous dans votre profil pour effectuer cette confirmation. \r\n\r\nCette mesure est prise dans un soucis de transparence et de légalité vis-à-vis de la CNIl. \r\n\r\nCliquez sur le liens ci-dessous pour plus d\'informations. ','02b170c026',1130260860,1130886000,214,'Amélie','forum/viewtopic.php?t=2345',0,1);
INSERT INTO `famille_maj` VALUES (19,'Ajout du site \"Autrepart\"','Ajout du site \"Autrepart\" dans la catégorie \"Jean-Jacques Goldman\" de la rubrique liens. Un site original pleins de surprises ! ','924c5f5c71',1130933230,1129327200,214,'Amélie','liens/view_cate.php?cate_id=1',0,1);
INSERT INTO `famille_maj` VALUES (20,'Vidéographie','Ajout de toute la tracklist de la vidéographie de JJG par Goldmanikou.','feedcf6339',1131792224,1130623200,5,'Boris','disco/goldman_albums_video.html',0,1);
INSERT INTO `famille_maj` VALUES (21,'Ajout du site les-enfoires.net','Ajout par Amélie du site les-enfoires.net, dans la catégorie \"les Enfoirés\" de la rubrique lien. Un site indispensable pour avoir les bonnes infos. ','907def2cd0',1132045660,1132009200,214,'Amélie','liens/view_cate.php?cate_id=3',0,1);
INSERT INTO `famille_maj` VALUES (23,'Images interdites en signature du forum.','A partir du 1er décembre 2005, [b:d533751903]il ne sera plus possible d\'afficher une image dans sa signature sur le forum.[/b:d533751903] Cette décision a été prise suite à l\'importance grandissante des bannières, qui pourrait au final gêner la lecture du forum. \r\nIl est toujours possible d\'afficher un lien texte dans sa signature, et les bannières sont toujours les bienvenues dans la rubrique \"Liens\". \r\nPlus d\'informations sur le lien ci-dessous.  ','d533751903',1133009662,1133910000,214,'Amélie','forum/viewtopic.php?t=2386',0,1);
INSERT INTO `famille_maj` VALUES (25,'Disco > Les enfoirés','Ajout par boris d\'une nouvelle rubrique dans la discographie dédiée aux Enfoirés. 5 albums sont déja disponibles. ','472bc287ce',1133121973,1133737200,725,'Goldmanikou','disco/les_enfoires_albums_live_66.html',0,1);
INSERT INTO `famille_maj` VALUES (27,'Le roman de Daniel Balavoine.','Ajout par Olivier du livre [i:4ffe76d80b]Le roman de Daniel Balavoine[/i:4ffe76d80b], dans la rubrique \"bibliothèque\" (catégorie \"Pour les autres\"). Jean-Jacques Goldman y a écrit la préface (plus d\'infos dans les news). ','4ffe76d80b',1136374454,1136070000,214,'Amélie','jjg/ll83-pour-les-autres-le-roman-de-daniel-balavoine.html',0,1);
INSERT INTO `famille_maj` VALUES (28,'Enfoirés 2006 : échanges de places.','Après la grande course aux places pour les concerts des Enfoirés 2006, place aux échanges. \r\n\r\n[b:621cd1957a]Le forum \"JJG en live\"[/b:621cd1957a] (voir lien ci-dessous) vous accueille pour tous achats/ventes/échanges de places pour ces concerts. Nous vous rappelons que, conformément à la charte, le prix maximum pour une place est de 45€ (prix commercial). Plus d\'infos [famurl=http://www.jjgfamille.com/forum/viewtopic.php?t=2419]ICI[/famurl]. \r\n\r\nPar ailleurs, une pétition a été mise en place par Vanille, pour protester contre la vente aux enchère de ces places sur des sites spécialisés. Plus d\'infos [famurl=http://www.jjgfamille.com/forum/viewtopic.php?t=2417]ICI[/famurl]. ','621cd1957a',1136803678,1136415600,214,'Amélie','forum/viewforum.php?f=34',0,1);
INSERT INTO `famille_maj` VALUES (29,'Site \"JJG et les Pauliniens\".','Ajout dans la rubrique Liens (catégorie \"Ses collaborateurs\") du site Jean-Jacques Goldman et les Pauliniens. De belles interviews très intéressantes vous attendent.  ','3499d39091',1136970107,1137970800,214,'Amélie','liens/view_cate.php?cate_id=2',0,1);
INSERT INTO `famille_maj` VALUES (30,'Photos concert Enfoirés.','Ajout par Compositeur, utilisateur du forum, de cinq photos du concert \"Le train des Enfoirés\", date du 19 janvier 2005 à Clermont-Ferrand. ','1179ac0134',1138008414,1138143600,214,'Amélie','tournees/concerts.php?concert_id=381#',0,1);
INSERT INTO `famille_maj` VALUES (31,'Ajout de Maurane dans les participations','Ajout dans les participations de la discographie de Maurane et ses albums studio \"Quand l\'humain danse\" ainsi que live \"L\'heureux tour\" dont 4 titres sont écrits et composés par Jean-Jacques Goldman.','81f79511ca',1138129588,1138316400,5,'Boris','disco/goldman_album_540_quand_l_humain_danse.html',0,1);
INSERT INTO `famille_maj` VALUES (32,'Ajout du livret RTL2 dans la bibliothèque','Ajout du livre de photos \"Concerts très très privés\" dans la bibliothèque.','d08e389d38',1138258048,1136761200,5,'Boris','jjg/ll84-et-aussi-rtl-2-concerts-tr-egrave-s-tr-egrave-s-prives.html',0,1);
INSERT INTO `famille_maj` VALUES (33,'Soirée chat du 26/01 : Logs disponibles','Ajout de l\'intégralité des logs du chat qui a eu lieu le jeudi 26 janvier. Le thème portait sur les droits d\'auteurs et le téléchargement.\r\n\r\n[i:fe5181eecc](Merci à Pierre pour l\'enregistrement)[/i:fe5181eecc]','fe5181eecc',1138484167,1138143600,5,'Boris','chat/soirees.php?mode=archives',0,1);
INSERT INTO `famille_maj` VALUES (34,'Participation : Ray Charles.','Ajout par Nico de l\'artite Ray Charles dans les participations, avec sa chanson Pacific Palisades, composée par Goldman. Paroles et références. ','6e20038b9c',1138783600,1138748400,214,'Amélie','disco/goldman_chanson_549_pacific_palisades.html',0,1);
INSERT INTO `famille_maj` VALUES (35,'Nouveau concours : à gagner le livre \"Concerts très très privés\".','[img:20c900a222]http://www.jjgfamille.com/images/news/livrets_rtl2_mini.JPG[/img:20c900a222]\r\nLe site famille organise un [b:20c900a222]nouveau concours[/b:20c900a222] ! \r\nA gagner cette fois-ci ; trois exemplaires du nouveau livre parut il y a quelques jours : [i:20c900a222]Concerts très très privés[/i:20c900a222] (plus d\'infos sur ce livre dans les [famurl=http://www.jjgfamille.com/actu/n177-goldman-dans-le-nouveau-livre-concerts-tres-tres-prives.html]>NEWS[/famurl]). \r\n\r\nLe concours débutera le [b:20c900a222]lundi 13 février[/b:20c900a222] et se terminera le dimanche 5 mars 2006. Il consistera en un quizz de 4-5 questions sur le livre et le concert \"très très privé\" de Goldman en 2002. Les bonnes réponses peuvent se trouver sur le site famille et le site des éditions [url=http://www.scali.net]>SCALI[/url], qui éditent ce livre. \r\n\r\n[b:20c900a222]Vous pouvez participer dès à présent[/b:20c900a222] en cliquant [famurl=http://www.jjgfamille.com/more/concours_goldman.html]>ICI[/famurl]. \r\n\r\nMerci aux éditions [url=http://www.scali.net]SCALI[/url] pour leur coopération, et qui nous ont fournit ces trois livres. [url=http://www.scali.net][img:20c900a222]http://www.jjgfamille.com/scalie.gif[/img:20c900a222][/url]. \r\n ','20c900a222',1139220625,1140390000,214,'Amélie','more/concours_goldman.html',0,1);
INSERT INTO `famille_maj` VALUES (36,'Récit de concert \"Un tour ensemble\".','Ajout par Nady (utilisatrice du site) d\'un récit de concert de la tournée \"Un tour ensemble\", date du 30 avril 2002 au Zénith de Caen. ','b3cd91b8c3',1140597632,1140735600,214,'Amélie','tournees/concerts.php?concert_id=14',0,1);
INSERT INTO `famille_maj` VALUES (37,'Le village des Enfoirés','Ajout dans la rubrique tournée de toutes les dates du [i:80956da83e]Village des Enfoirés[/i:80956da83e]. N\'hésitez pas à y laisser un commentaire et/ou des photos.  ','80956da83e',1140715691,1141167600,214,'Amélie','tournees/tournees.php?tournee_id=27&cate_id=5',0,1);
INSERT INTO `famille_maj` VALUES (38,'Le village des enfoirés : suite','Mise à jour des rubriques discographie et tournées pour ajouter l\'intégralité des titres des enfoirés 2006 (avec les références aux version originales).\r\n\r\nDe plus, un récit de concert a été posté par autom1. Merci à elle.','f7e0efeeb9',1140825117,1140822000,5,'Boris','disco/goldman_album_545_le_village_des_enfoires.html',0,1);
INSERT INTO `famille_maj` VALUES (39,'Enfoirés 2006 : photos et récits de concerts','Ajout de plusieurs photos et récits de concerts d\'utilisateurs dans la rubrique Tournées du site.\r\n\r\nMerci à eux !','75d9d69911',1141256198,1143842400,5,'Boris','tournees/tournees.php?tournee_id=27&cate_id=5',0,1);
INSERT INTO `famille_maj` VALUES (40,'Fin du concours \"concert très très privé\".','Après trois semaines de jeux, le quatrième concours de famille s\'est terminé hier soir ! Trois gagnants ont été tiré au sort parmis toutes les bonnes réponses au quizz. Il s\'agit de [b:fd4d579b91]fab-, Jarc et Mi[/b:fd4d579b91]. Ces trois personnes remportent chacun un exemplaire du livre [i:fd4d579b91]Concert très très privés RTL2[/i:fd4d579b91]. \r\n\r\nMerci aux éditions [url=www.scali.net]Scali[/url].  ','fd4d579b91',1141633841,1143756000,214,'Amélie','more/concours_goldman.html',0,1);
INSERT INTO `famille_maj` VALUES (41,'Nouveaux smileys','Petit changement au niveau des smileys du forum. Certains, peu utilisés, ont été supprimé. D\'autres ont fait leur apparition. Venez les découvrir sur le forum. ','95aeef71f8',1147515151,1150322400,214,'Amélie','forum/viewtopic.php?t=2264',0,1);
INSERT INTO `famille_maj` VALUES (42,'Livre \"la vie à en mourir\".','[img:dc1a899d55]http://multimedia.fnac.com/multimedia/images_produits/grandes110/1/0/2/9782757800201.gif[/img:dc1a899d55]\r\n\r\nAjout par Olivier d\'un nouveau livre dans la bibliothèque (rubrique J.J.Goldman), dans la catégorie \"Pour les autres\". Il s\'agit de [i:dc1a899d55]La vie à en mourir : Lettres de fusillés, 1941-1944[/i:dc1a899d55], regroupant 120 lettres de résistants condamnés à mort. \r\nJean-Jacques Goldman a signé la préface de la nouvelle édition du livre. \r\n\r\n\r\n		\r\n		','dc1a899d55',1148639829,1151618400,214,'Amélie','jjg/ll85-pour-les-autres-la-vie-a-en-mourir-lettres-de-fusilles-1941-1944.html',0,1);
INSERT INTO `famille_maj` VALUES (43,'Ajout des RDFs anniversaires','Ajout des 2 RDFs anniversaire (Paris / Lyon) à l\'occasion des 5 ans de famille.\r\n\r\nVenez vite vous y inscrire pour être tenu au courant de l\'organisation de ces dernières !','f79cf7a8d9',1157027834,1160863200,5,'Boris','famille/rdf.php',0,1);
INSERT INTO `famille_maj` VALUES (44,'Photos et résumés de RDFs.','Ajouts de photos et résumés de la RDF anniversaire chez Manue le 7 octobre 2006. RDf pour fêter les cinq ans du site famille. ','43ae8ef04b',1160392121,1166137200,214,'Amélie','famille/view_rdf.php?rdf_id=20',0,1);
INSERT INTO `famille_maj` VALUES (45,'Nouveau livre de Patrick Amine sur Goldman','Ajout de l\'ouvrage \"Jean-Jacques Goldman : un monde à part\" de Patrick Amine dans la Bibliothèque','4fa59a141e',1167562042,1177624800,5,'Boris','jjg/ll81-biographies-jean-jacques-goldman-un-monde-a-part.html',0,1);
INSERT INTO `famille_maj` VALUES (46,'Icones de Bureau Windows','Alisonne (nouvelle membre récemment inscrite sur JJGfamille) a complété la rubrique Goodies avec 4 fichiers icône pour personnaliser votre bureau Windows avec la tête de notre JJG national.\r\n\r\nMerci à elle !','7e4980680c',1167695744,1173913200,5,'Boris','more/view_cate.php?cate_id=15',0,1);
INSERT INTO `famille_maj` VALUES (47,'Galerie Vidéos','Après quelques mois de calme plat, voici une petite nouveauté sur le site : la galerie vidéos !!!!\r\n\r\nCette rubrique vous permet de partager les vidéos de Jean-Jacques Goldman présentes sur le célèbre site YouTube (une adaptation pour DailyMotion est en cours).\r\n\r\nN\'hésitez donc pas à ajouter vos trouvailles (voire même vos créations) et en discuter sur le forum (chaque vidéo postée créant automatiquement un sujet sur le forum).','dfe55bda21',1194819466,1200006000,5,'Boris','medias/video.php',0,1);
INSERT INTO `famille_maj` VALUES (48,'Nouvel ouvrage : Jean-Jacques Goldman - Un homme comme il faut','Retrouvez le dernier ouvrage qui lui est consacré','02f121ba21',1288568266,1293750000,5,'Boris','jjg/ll87-biographies-jean-jacques-goldman-un-homme-bien-comme-il-faut.html',0,1);
