INSERT INTO `disco_albums` VALUES (493,'1 fille & 4 types',23,'l\'album',20031013,'Tout d\'abord, un grand merci à mes 4 types : Gildas Arzel > Erick Benzi > Jean-Jacques Goldman > Jacques Veneruso.\r\n<br>\r\n<br>\"Chanter en Français pour moi, c\'est d\'abord un retour aux sources, le bonheur de retrouver ma langue natale et \'mes pays\' : le Quebec et la France qui m\'ont tant donné.\r\n<br>Chanter en Français c\'est aussi cette équipe. Les auteurs, les musiciens avec lesquels j\'ai tant de plaisir à partager! Et c\'est evidemment la perspective de retrouver l\'energie et la chaleur particulières du public francophone. \r\n<br>Merci à vous d\'avoir toujours su si bien accueillir la voix de la petite Quebecoise et les mots de ses auteurs\r\n<br> <br>                                Céline...  \"','','885a1231ab','Y','N','N','N','N','N','N','N','N','N','Y','1474519',0);
