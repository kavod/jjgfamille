INSERT INTO `media_emission` VALUES (21,6,'Attaques, critiques, rumeurs , Goldman répond','Trop secret? Trop riche? Trop commercial?Alors qu’il vient de sortir un nouvel album, l’homme le plus puissant de la chanson française énerve encore beaucoup de monde. Sophie Delassein l’a rencontré','','report',0,20011220,'',1,6,'MMF',0,0,0);
INSERT INTO `media_emission` VALUES (22,31,'Le grand bal de Jean-Jacques Goldman','Pour son nouvel album, l\'auteur-compositeur aux doigts d\'or convoque rythmes modernes et anciens, fanfares, chorales et ensembles traditionnels, avec un but explicite: inviter à la danse. `Chansons pour les pieds´, il les appelle','','report',18,20011210,'',1,5,'Boris',0,0,0);
INSERT INTO `media_emission` VALUES (23,32,'Goldman, amoureux de la vie !','On pourrait parler des heures, des années avec lui. Des chansons et de tout ce qu\'il y a autour.<BR>Jean-Jacques Goldman a pratiquement toujours les bonnes réponses. Un plaisir réalisé à Paris.','','report',18,20020202,'',1,13,'Effde',0,0,0);
