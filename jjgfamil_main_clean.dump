INSERT INTO `media_emission` VALUES (214,4,'Des classiques de la chanson fran&ccedil;aise d&eacute;tourn&eacute;s sur la Toile.','Information RTL : le parquet de Paris vient d\'ouvrir une enquête préliminaire pour incitation à la haine raciale après une plainte de Gérard Lenorman. Le chanteur a découvert qu\'une de ses chansons avait été détournée par un groupe néo-nazi des années 80.','9446b1ad91','autre',0,20080818,'',1,59,'Olivier',0,20080819,20080919);
INSERT INTO `media_emission` VALUES (215,106,'G&eacute;rard Lenorman plagi&eacute; par des nazis.','Un groupe néonazi a détourné sa «Ballade des gens heureux» en une rengaine ultraraciste. Gérard Lenorman porte plainte. JEAN-JACQUES GOLDMAN et Gilbert Montagné ont, eux aussi, été parodiés\r\n\r\n','871b5039c2','Itw',0,20080818,'23h00',1,59,'Olivier',0,20080819,20080919);
INSERT INTO `media_emission` VALUES (216,107,'Le retour de Jean-Jacques Goldman?','Le chanteur préféré des Français n\'a pas sorti d\'album depuis 7 ans maintenant et de nombreux fans se mobilisent pour qu\'il revienne en solo, le plus tôt possible.\r\n\r\n  ','b771c765f3','autre',0,20080916,'11h00',1,59,'Olivier',0,20080917,20081016);
INSERT INTO `media_emission` VALUES (217,108,'Jean-Jacques Goldman : Ses vinyles bient&ocirc;t r&eacute;&eacute;dit&eacute;s!','Le 10 novembre 2008 sortira un coffret que de nombreux fans de Jean-Jacques Goldman vont s’empresser d’acheter...','aa6fbc56df','autre',0,20081025,'01h20',1,59,'Olivier',0,20081025,20081125);
INSERT INTO `media_emission` VALUES (218,109,'Cet Enfoir&eacute; de Michael Jones','Michael Jones sera sur scène à Liège pour les Restos du Coeur le 15mai. Il revisitera le répertoire de Jean-Jacques Goldman avec Sunset Boulevard et le choeur de l\'Opéra royal de Wallonie.','266e41b0c2','Itw',0,20080425,'',1,5,'Boris',0,20090327,20090525);
