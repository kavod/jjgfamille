INSERT INTO `disco_songs` VALUES (386,'Dîner asiatique','Dînerasiatique','N',0,0,'pas de paroles','Basse : Guy Delacroix \r<br>Batterie : Claude Salmieri\r<br>Choeurs : Michael Jones \r<br>Clarinette : Pascal Moragues \r<br>Claviers : Roland Romanelli \r<br>Guitares : Jean-Jacques Goldman, Michael Jones, Patrice Tison \r<br>Orgues : Jean-Jacques Goldman \r<br>Piano : Claire Désert \r<br>Saxophone : Patrick Bourgoin ','302807bf12','','N','N','N');
INSERT INTO `disco_songs` VALUES (387,'Thème de Lisa','ThèmedeLisa','N',0,0,'Verts ou bruns à la fois\r\nVerts ou bruns quand elle aime ou n\'aime pas\r\nDans les yeux de Lisa\r\nMoi, je lisais ma vie, autrefois\r\n\r\nMoi, semaines après mois,\r\nLoin du reflet de ses yeux verts et bruns\r\nMoi, j\'existe un peu moins\r\nSans ces yeux là, \r\nJe ne me vois pas.\r\nLisa','Basse : Guy Delacroix \r<br>Batterie : Claude Salmieri\r<br>Choeurs : Michael Jones \r<br>Clarinette : Pascal Moragues \r<br>Claviers : Roland Romanelli\r<br>Guitares : Jean-Jacques Goldman, Michael Jones, Patrice Tison \r<br>Orgues : Jean-Jacques Goldman \r<br>Piano : Claire Désert \r<br>Saxophone : Patrick Bourgoin ','a1dd6fd8c2','','N','N','N');
