  `part_author` varchar(255) NOT NULL DEFAULT '',
  `part_adress` varchar(255) NOT NULL DEFAULT '',
  `comment` text NOT NULL,
  `bbcode_uid` varchar(10) NOT NULL DEFAULT '',
  `ordre` int(2) unsigned NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  KEY `album_id` (`album_id`),
  KEY `bbcode_uid` (`bbcode_uid`),
  KEY `duree` (`duree`),
  KEY `id` (`id`),
  KEY `ordre` (`ordre`),
  KEY `part_adress` (`part_adress`),
  KEY `part_author` (`part_author`),
  KEY `part_type` (`part_type`),
  KEY `song_id` (`song_id`)
) ENGINE=MyISAM AUTO_INCREMENT=1400 DEFAULT CHARSET=latin1;
