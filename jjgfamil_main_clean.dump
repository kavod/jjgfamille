INSERT INTO `media_emission` VALUES (10,8,'Concert Celine Dion au stade de France','Retransmission du concert de Celine Dion au Stade de France (ne louppez pas le passage de JJ pour s\'il suffisait d\'aimer <img src=\"/forum/images/sm03.gif\">  ).<BR>En simultané sur RTL2 accompagné d\'une journée spéciale Celine Dion.','','report',0,20020102,'23H',1,5,'Boris',0,0,0);
