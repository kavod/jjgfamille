INSERT INTO `disco_songs_albums` VALUES (591,66,421,'5:42','','','','[i:af45e737b6]Des heures passées à improviser sur cette suite d\\\'accords... Cette chanson m\\\'a volé des nuits[/i:af45e737b6]','af45e737b6',1);
INSERT INTO `disco_songs_albums` VALUES (592,69,421,'4:45','','','','[i:993f1511aa]Textes triste sur musique gaie... Parce qu\\\'il y a toujours un peu de tristesse dans la joie. Et vice versa...[/i:993f1511aa]','993f1511aa',2);
INSERT INTO `disco_songs_albums` VALUES (593,64,421,'3:54','','','','[i:d7fbb02e14]Enregistrement compliqué. Finalement il fallait légèrement changer le tempo à chaque couplet. Sinon ça ne collait pas. Peut-être la chanson qui justifie le mieux notre trio.[/i:d7fbb02e14]','d7fbb02e14',3);
INSERT INTO `disco_songs_albums` VALUES (594,62,421,'4:59','','','','[i:95f50bd170]Est-il bien utile d\\\'écrire des choses comme ça ?[/i:95f50bd170]','95f50bd170',4);
INSERT INTO `disco_songs_albums` VALUES (595,65,421,'4:17','','','','[i:ace7889800]Séquence de synthé sous un vieuw rock... J\\\'aime bien les sacrilèges.[/i:ace7889800]','ace7889800',5);
INSERT INTO `disco_songs_albums` VALUES (596,71,421,'8:37','Tablatures','','','[i:6f3c157fed]Enregistré en direct. Pénombre. SOuvenir de Basile et Patrice, deux guitaristes si respectueux l\'un de l\'autre... Un super moment[/i:6f3c157fed]','6f3c157fed',6);
INSERT INTO `disco_songs_albums` VALUES (597,70,421,'3:59','','','','[i]Un peu \\','4f0ef354bd',7);
INSERT INTO `disco_songs_albums` VALUES (598,1,421,'4:09','','','','[i:244f3a90fd]En do mineur mineur au lieu du ré d\\\'origine. J\\\'ai peut-être finie par muer...[/i:244f3a90fd]','244f3a90fd',8);
INSERT INTO `disco_songs_albums` VALUES (599,51,421,'3:47','','','','[i:78302ad667]Le texte qui nous va si bien quand on part en tournée ![/i:78302ad667]','78302ad667',9);
INSERT INTO `disco_songs_albums` VALUES (600,77,421,'6:15','','','','[i:829ece7f88]Moscou, septembre 93.\r<br>Nous marchons dans une caserne immense. Vide. AU loin nous les entendons répéter. Les voix se rapprochent peu à peu. Inoubliable.[/i:829ece7f88]','829ece7f88',10);
INSERT INTO `disco_songs_albums` VALUES (601,76,421,'4:41','','','','[i:7e0ca45d29]Désormais indissociable des images. C\\\'est devenu la bande son du clip, du film sur scène, la chanson du nouveau né zaïrois[/i:7e0ca45d29]','7e0ca45d29',11);
INSERT INTO `disco_songs_albums` VALUES (602,80,421,'4:18','','','','Londres, 6H du mat.\r<br>Exténués, fin du clip. Ce soir, concert à Bruxelles...[/i]','99ab74c669',12);
