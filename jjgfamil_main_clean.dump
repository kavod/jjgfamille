INSERT INTO `media_emission` VALUES (198,97,'Goldman, la plus belle cash-machine du show-biz.','Chanteur exigeant, gros bosseur, compositeur perfectionniste... Le business de Jean-Jacques Goldman est florissant. Derrière son look d\'étudiant,  JJG se révèle un redoutable homme d\'affaires, diplômé de l\'Edhec et multimillionaire. \r\n\r\nUn reportage de 2 page ssur les revenus de JJG, son mode de vie, sa façon de travailler, l\'investissement de son argent et ses dons. ','9dd1206618','report',0,20060330,'',1,214,'Amélie',0,20060330,20060501);
